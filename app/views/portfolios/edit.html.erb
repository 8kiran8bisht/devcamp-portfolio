<h1>Edit Portfolio</h1>
<%= form_for(@portfolio_items) do |form| %>
 

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
   <div class="field">
    <%= form.label :sbutitle%>
    <%= form.text_field :sbutitle %>
  </div>

  <div class="field">
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
